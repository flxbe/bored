{
  "name": "bored",
  "version": "1.0.0",
  "description": "A data modeling language",
  "main": "index.js",
  "scripts": {
    "start:db": "docker run --rm -e MYSQL_USER=root -e MYSQL_PASSWORD=root -e MYSQL_DATABASE=db -p 3306:3306 -it mysql/mysql-server:5.6",
    "generate": "ts-node ./src/generator/index.ts",
    "build": "npm run generate && tsc -p .",
    "test": "npm run generate && jest --runInBand"
  },
  "author": "Felix Bernhardt",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "mysql": "^2.17.1",
    "prettier": "^1.18.2",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "@types/prettier": "^1.18.3",
    "@types/validator": "^10.11.3",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/generated/"
    ]
  }
}
