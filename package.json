{
  "name": "bored",
  "version": "1.0.0",
  "description": "A data modeling language",
  "main": "index.js",
  "scripts": {
    "start:db": "docker run --rm -e POSTGRES_USER=root -e POSTGRES_PASSWORD=root -e POSTGRES_DB=db -p 5432:5432 -it postgres",
    "generate": "ts-node ./src/generator/index.ts",
    "build": "npm run generate && tsc -p .",
    "test": "npm run generate && jest --runInBand"
  },
  "author": "Felix Bernhardt",
  "license": "MIT",
  "dependencies": {
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "prettier": "^1.18.2",
    "sequelize": "^5.19.5"
  },
  "devDependencies": {
    "@types/faker": "^4.1.7",
    "@types/jest": "^24.0.21",
    "@types/node": "^12.12.5",
    "@types/prettier": "^1.18.3",
    "@types/validator": "^10.11.3",
    "faker": "^4.1.0",
    "jest": "^24.9.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/generated/"
    ]
  }
}
